<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="ros.android.pr2jointteleop" android:versionCode="1"
	android:versionName="1.0">
	<uses-sdk android:minSdkVersion="9" />

	<application android:icon="@drawable/pr2jointteleop" android:label="@string/app_name">

		<activity android:name=".Pr2JointTeleop" android:label="@string/app_name"
			android:screenOrientation="landscape" android:configChanges="orientation|keyboardHidden">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
			<intent-filter>
				<action android:name="ros.android.pr2jointteleop.Pr2JointTeleop" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
	</application>
	
	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
	<uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
</manifest>
